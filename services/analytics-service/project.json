{
  "name": "analytics-service",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "services/analytics-service/src",
  "projectType": "application",
  "targets": {
    "dev": {
      "command": "bun --watch src/index.ts",
      "options": {
        "cwd": "services/analytics-service"
      }
    },
    "build": {
      "command": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile ../../dist/analytics-service src/index.ts",
      "options": {
        "cwd": "services/analytics-service"
      }
    },
    "start": {
      "command": "../../dist/analytics-service",
      "options": {
        "cwd": "services/analytics-service"
      }
    }
  }
}
